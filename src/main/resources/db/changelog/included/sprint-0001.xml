        <createIndex indexName="IDX_USER_ENGINEER_USERNAME"
                     schemaName="DEPDATA"
                     tableName="user"
                     unique="true">
            <column name="username"/>
        </createIndex>
        <createIndex indexName="IDX_USER_MATRICULE"
                     schemaName="DEPDATA"
                     tableName="user"
                     unique="true">
            <column name="matricule"/>
        </createIndex>
        <createIndex indexName="IDX_USER_EMAIL"
                     schemaName="DEPDATA"
                     tableName="user"
                     unique="true">
            <column name="email"/>
        </createIndex>
        <createIndex indexName="IDX_USER_PHONE"
                     schemaName="DEPDATA"
                     tableName="user"
                     unique="true">
            <column name="phone"/>
        </createIndex>
    </changeSet>

    <changeSet id="0001-03" author="RBOU">
        <!--        CATEGORY Indexes           -->
        <createIndex indexName="IDX_CATEGORY_LABEL"
                     schemaName="DEPDATA"
                     tableName="CATEGORY"
                     unique="true">
            <column name="LABEL"/>
        </createIndex>
        <createIndex indexName="IDX_CATEGORY_NEXT_CATEGORY_ID"
                     schemaName="DEPDATA"
                     tableName="CATEGORY"
                     unique="true">
            <column name="NEXT_CATEGORY_ID"/>
        </createIndex>
        <createIndex indexName="IDX_CATEGORY_PREVIOUS_CATEGORY_ID"
                     schemaName="DEPDATA"
                     tableName="CATEGORY"
                     unique="true">
            <column name="PREVIOUS_CATEGORY_ID"/>
        </createIndex>
        <createIndex indexName="IDX_CATEGORY_orderNum"
                     schemaName="DEPDATA"
                     tableName="CATEGORY"
                     unique="true">
            <column name="orderNum"/>
        </createIndex>
        <!--        SUB_CATEGORY Indexes           -->
        <createIndex indexName="IDX_SUB_CATEGORIES_LABEL"
                     schemaName="DEPDATA"
                     tableName="SUB_CATEGORIES"
                     unique="true">
            <column name="LABEL"/>
        </createIndex>
        <createIndex indexName="IDX_SUB_CATEGORIES_CATEGORY_ID"
                     schemaName="DEPDATA"
                     tableName="SUB_CATEGORIES"
                     unique="true">
            <column name="CATEGORY_ID"/>
        </createIndex>
    </changeSet>

</databaseChangeLog>